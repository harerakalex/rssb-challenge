{
  "name": "rssb-challenge",
  "version": "1.0.0",
  "description": "API to analyse file",
  "main": "./src/server.js",
  "author": "Carlos Harerimana",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "start": "node .",
    "dev": "nodemon",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "create:db": "node_modules/.bin/sequelize db:create rssbdb",
    "drop:db": "node_modules/.bin/sequelize db:drop rssbdb"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.13",
    "@types/sequelize": "^4.28.10",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.12",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "winston": "^3.3.3",
    "xlsx": "^0.17.0"
  }
}
